{
  "name": "objio",
  "version": "0.0.20",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:xegax/objio.git",
  "scripts": {
    "prepare": "tsc -p tsconfig.publish.json",
    "postpublish": "npm run clean",
    "dist": "tsc",
    "dev": "npm run clean-build && tsc -p tsconfig.publish.json -outDir build && shx cp package.json build",
    "test": "node_modules/.bin/mocha",
    "dev-test": "npm run clean-build && tsc -p tsconfig.json && npm run test",
    "clean": "npm run clean-index && npm run clean-src && npm run clean-test",
    "clean-index": "shx rm -rf index.d.ts index.js index.js.map server.d.ts server.js server.js.map",
    "clean-src": "shx rm -rf src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "clean-test": "shx rm -rf test/**/*.js test/**/*.js.map",
    "clean-build": "shx rm -rf build/**/*"
  },
  "author": "xega",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/d3": "^4.9.0",
    "@types/es6-shim": "^0.31.34",
    "@types/lodash": "^4.14.93",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.7",
    "@types/sqlite3": "^3.1.3",
    "awesome-typescript-loader": "^3.2.1",
    "bluebird": "^3.5.0",
    "chai": "^4.1.2",
    "d3": "^4.10.0",
    "lodash": "^4.17.4",
    "mocha": "^5.1.1",
    "tslint": "^3.15.1",
    "typescript": "^2.4.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "sqlite3": "^4.0.1"
  }
}
